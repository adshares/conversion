{
    "name": "adshares/ads",
    "description": "ADS tools",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Adshares\\Ads\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "database/"
        ]
    },
    "require": {
        "php": ">=7.2",
        "laravel/lumen-framework": "~5.6",
        "vlucas/phpdotenv": "~2.2",
        "sc0vu/web3.php": "dev-master",
        "adshares/ads-client": "dev-master",
        "phpmailer/phpmailer": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "fzaninotto/faker": "~1.0",
        "mockery/mockery": "~1.0"
    },
    "config": {
        "preferred-install": "dist",
        "bin-dir": "bin/vendor"
    }
}
